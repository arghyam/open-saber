          "type": "string",
          "title": "Longitude"
        },
        "elevation" : {
          "$id": "#/properties/elevation",
          "type": "string",
          "title": "Elevation"
        },
        "crtdDttm": {
          "$id": "#/properties/crtdDttm",
          "type": "string",
          "title" : "Created Date"
        },
        "updtDttm": {
          "$id": "#/properties/updtDttm",
          "type": "string",
          "title" : "Updated Date"
        }
