      "required": [
        "springCode",
        "springName"
      ],
      "additionalProperties": true,
      "properties": {
        "springID": {
          "$id": "#/properties/springCode",
          "type": "string",
          "title": "SpringCode"
        },
        "springName": {
          "$id": "#/properties/springName",
          "type": "string",
          "title": "Spring name"
        },
        "organization": {
          "$id": "#/properties/organization",
          "type": ["string", "null"],
          "title": "Spring added by "
        },
        "uploadedBy": {
          "$id": "#/properties/uploadedBy",
          "type": "string",
          "title": "Uploaded By"
        },
        "tenantId": {
          "$id": "#/properties/tenantId",
          "type": "string",
          "title" : "Tenant ID"
        },
        "village": {
          "$id": "#/properties/village",
